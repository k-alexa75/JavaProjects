      <TextField fx:id="textFieldUsername" layoutX="169.0" layoutY="92.0" />
      <Label layoutX="90.0" layoutY="96.0" prefHeight="20.0" prefWidth="69.0" text="Username" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="93.0" layoutY="135.0" text="Password" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
